<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="net.sf.anathema.character.models" name="%extension-point.name.charactermodels" schema="schema/net.sf.anathema.character.models.exsd"/>
   <extension
         point="net.sf.anathema.repository.itemtypes">
      <itemtype
            icon="icons/TabCharacter16.png"
            id="net.sf.anathema.itemtype.Character"
            project="Character"
            type="%itemtype.type.character"
            untitledName="%itemtype.untitled.character"
            viewElementFactoryClass="net.sf.anathema.character.core.repository.CharacterViewElementFactory">
      </itemtype>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="net.sf.anathema.character.description.CharacterDescriptionEditor"
            icon="icons/TabCharacter16.png"
            id="net.sf.anathema.character.description.editor"
            name="%editor.name.characterdescription">
      </editor>
      <editor
            class="net.sf.anathema.character.attributes.AttributesEditor"
            icon="icons/TabCharacter16.png"
            id="net.sf.anathema.character.attributes.editor"
            name="%editor.name.attributes">
      </editor>
   </extension>
   <extension
         point="net.sf.anathema.character.models">
      <model
            id="net.sf.anathema.character.description.model"
            modelFactory="net.sf.anathema.character.description.DescriptionModelFactory"
            viewElementFactory="net.sf.anathema.character.description.CharacterDescriptionViewElementFactory">
      </model>
      <model
            id="net.sf.anathema.character.attributes.model"
            modelFactory="net.sf.anathema.character.attributes.AttributesFactory"
            viewElementFactory="net.sf.anathema.character.attributes.AttributesViewElementFactory">
      </model>
      <model
            id="net.sf.anathema.character.basics.model"
            modelFactory="net.sf.anathema.character.basics.BasicsFactory">
      </model>
   </extension>

</plugin>
