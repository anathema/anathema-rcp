         point="org.eclipse.ui.editors">
      <editor
            class="net.sf.anathema.campaign.conflictweb.ConflictWebEditor"
            extensions="conflict"
            icon="icons/TabConflictWeb16.png"
            id="net.sf.anathema.editors.conflictweb"
            name="%conflictWebEditor.name">
