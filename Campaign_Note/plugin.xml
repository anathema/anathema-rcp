<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="net.sf.anathema.repository.itemtypes">
      <itemtype
            file-extension="not"
            icon="icons/TabNotes16.png"
            id="net.sf.anathema.itemtype.Note"
            project="Notes"
            type="%itemtype.type.notes"
            untitledName="%itemtype.untitled.note"
            viewElementFactoryClass="net.sf.anathema.basics.repository.treecontent.itemtype.FileItemTypeViewElementFactory">
      </itemtype>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="net.sf.anathema.notes.popup"
            objectClass="net.sf.anathema.basics.repository.treecontent.itemtype.ItemTypeViewElement">
         <action
               class="net.sf.anathema.campaign.note.NewNoteActionDelegate"
               icon="icons/TabNotes16.png"
               id="net.sf.anathema.notes.popup.new"
               label="%itemtype.new.note">
         </action>
      </objectContribution>
      <viewerContribution
            id="EmptyContribution"
            targetID="EmptyContribution">
      </viewerContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="net.sf.anathema.editor.styledtext.StyledTextEditor"
            contributorClass="net.sf.anathema.editor.styledtext.StyledTextEditorActionBarContributor"
            extensions="not"
            icon="icons/TabNotes16.png"
            id="net.sf.anathema.campaign.note.noteeditor"
            name="%net.sf.anathema.campaign.note.noteeditor.name">
      </editor>
   </extension>

</plugin>
